msgid "|input_date| = Input date"
msgstr "|input_date| = Введення дати"

#: ../../scatter_plot.rst:87
msgid "|input_text| = Input text"
msgstr "|input_text| = Ведення тексту"

#: ../../scatter_plot.rst:89
msgid "|input_list| = List"
msgstr "|input_list| = Вибір елемента з переліку"

#: ../../scatter_plot.rst:91
msgid "|input_number| = Input number"
msgstr "|input_number| = Введення числа"

#: ../../scatter_plot.rst:93
msgid "|optional| = Optional"
msgstr "|optional| = Необов'язковий елемент"

#: ../../scatter_plot.rst:95
msgid "|project_save| = Configuration stored in the active project of QGIS"
msgstr "|project_save| = Налаштування зберігаються в активному проекті QGIS"

#: ../../scatter_plot.rst:97
msgid "|registry_save| = Configuration stored in QGIS registry"
msgstr "|registry_save| = Налаштування зберігаються в реєстрі QGIS"

